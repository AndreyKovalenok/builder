{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes":  ["error", "double"],
        "space-before-blocks": ["error", "always"],
        "object-curly-newline": ["error", "always"]
    }
}
